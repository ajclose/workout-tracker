<%= render "form" %>
<%= link_to "Delete Workout", workout_path, method: :delete, data: {confirm: "Are you sure you want to delete this workout?"} %>
<% if @workout.amrap %>
<h3><%= link_to 'AMRAP', workout_path(@workout) %></h3>
<%= link_to "Edit", edit_workout_amrap_path(@workout, @workout.amrap) %>
<p><%= @workout.amrap.time %> minutes </p>
<p><%= @workout.amrap.score %> rounds </p>
<% @workout.amrap.amrapmovements.each do |movement| %>
<p>RX: <%= movement.rx_reps %> <%= movement.rx_movement %> <%= movement.rx_weight %> <%= movement.rx_unit %></p>
<p>Scaled: <%= movement.scaled_reps %> <%= movement.scaled_movement %> <%= movement.scaled_weight %> <%= movement.scaled_unit %></p>
  <% end %>
<% end %>
